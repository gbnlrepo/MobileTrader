<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>homes</servlet-name>
        <servlet-class>apphome.homes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uAdmin</servlet-name>
        <servlet-class>adminInteface.uAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userAut</servlet-name>
        <servlet-class>login.userAut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>systemRight</servlet-name>
        <servlet-class>adminInteface.systemRight</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>assignmultipleuserGroup</servlet-name>
        <servlet-class>adminInteface.assignmultipleuserGroup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminComServlet</servlet-name>
        <servlet-class>adminCom.adminComServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>errorPage</servlet-name>
        <servlet-class>apphome.errorPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mandatory</servlet-name>
        <servlet-class>adminInteface.mandatory</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createCompany</servlet-name>
        <servlet-class>adminInteface.createCompany</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>generalConfig</servlet-name>
        <servlet-class>adminInteface.generalConfig</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>homes</servlet-name>
        <url-pattern>/homes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uAdmin</servlet-name>
        <url-pattern>/uAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userAut</servlet-name>
        <url-pattern>/userAut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>systemRight</servlet-name>
        <url-pattern>/systemRight</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>assignmultipleuserGroup</servlet-name>
        <url-pattern>/assignmultipleuserGroup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminComServlet</servlet-name>
        <url-pattern>/adminComServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>errorPage</servlet-name>
        <url-pattern>/errorPage</url-pattern>
    </servlet-mapping>
      <error-page>
        <error-code>405</error-code>
        <location>/errorPage</location>
    </error-page>
    
    <servlet-mapping>
        <servlet-name>mandatory</servlet-name>
        <url-pattern>/mandatory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createCompany</servlet-name>
        <url-pattern>/createCompany</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>generalConfig</servlet-name>
        <url-pattern>/generalConfig</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            304
        </session-timeout>
        <cookie-config>
            <http-only>true</http-only>
        </cookie-config>
    </session-config>
    <welcome-file-list>
        <welcome-file>homes</welcome-file>
    </welcome-file-list>
</web-app>
